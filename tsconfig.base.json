{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "module": "NodeNext",
    "noEmit": true,
    "noImplicitThis": true,
    "typeRoots": ["node_modules/@types"],
    "strict": false,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "target": "ESNext"
  },
  "typeAcquisition": {
    "enable": false
  }
}
